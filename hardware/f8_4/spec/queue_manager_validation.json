  "stats_fields": [
    "totalQueued",
    "totalCompleted",
    "totalFailed",
    "avgWaitMinutes",
    "currentJobId",
    "queueState"
  ],
  "features": [
    "job_enqueue",
    "priority_ordering",
    "drag_drop_reorder",
    "estimated_completion",
    "queue_state_management",
    "serial_serialization"
  ]
