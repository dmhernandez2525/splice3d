{
  "feature": "F8.4-queue-manager",
  "max_queue_size": 32,
  "priority_levels": 4,
  "queue_item_fields": [
    "jobId",
    "recipeName",
    "priority",
    "status",
    "estimatedMinutes",
    "actualMinutes",
    "position"
